{
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "eslint.enable": true,
  "eslint.alwaysShowStatus": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {},
  "files.insertFinalNewline": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "search.exclude": {
    ".git": true,
    ".build": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/coverage": true,
    "**/node_modules": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#2F2F08",
    "titleBar.activeBackground": "#42420B",
    "titleBar.activeForeground": "#FBFBE6"
  }
}
